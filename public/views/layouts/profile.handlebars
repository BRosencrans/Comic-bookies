{{#if isMyProfile}}
    <div class="profile-data">
        <h1>{{user.username}}'s page</h1>
        <p>{{user.bio}} </p>
        <button id="show-edit-form">Edit</button>
    </div>
    <form class="hide" id="edit-profile">
        <input type="text" id="edit-username" value="{{user.username}}">
